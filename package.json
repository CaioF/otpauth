{
  "name": "otpauth",
  "version": "9.0.0-beta.2",
  "description": "One Time Password (HOTP/TOTP) library for Node.js, Deno and browsers",
  "keywords": [
    "otp",
    "hotp",
    "totp",
    "one time password",
    "2fa",
    "2 factor",
    "two factor",
    "two-factor",
    "2step",
    "2 step",
    "two step",
    "two-step",
    "auth",
    "authenticator",
    "google authenticator"
  ],
  "author": "Héctor Molinero Fernández <hector@molinero.dev>",
  "license": "MIT",
  "homepage": "https://github.com/hectorm/otpauth",
  "repository": {
    "type": "git",
    "url": "https://github.com/hectorm/otpauth.git"
  },
  "bugs": {
    "url": "https://github.com/hectorm/otpauth/issues"
  },
  "type": "module",
  "types": "./dist/otpauth.d.ts",
  "main": "./dist/otpauth.node.cjs",
  "exports": {
    ".": {
      "types": "./dist/otpauth.d.ts",
      "node": {
        "import": "./dist/otpauth.node.mjs",
        "require": "./dist/otpauth.node.cjs"
      },
      "default": "./dist/otpauth.esm.js"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/",
    "src/",
    "types/"
  ],
  "scripts": {
    "lint": "eslint ./",
    "types:clean": "rimraf ./types/",
    "types:compile": "tsc",
    "types": "run-s types:clean types:compile",
    "build:clean": "rimraf ./dist/",
    "build:compile": "rollup -c",
    "build": "run-s build:clean build:compile",
    "docs:clean": "rimraf ./docs/",
    "docs:compile": "typedoc ./src/index.js --emit docs --readme none --theme default --out ./docs/",
    "docs": "run-s docs:clean docs:compile",
    "test:node:esm:unmin": "JSFILE=../dist/otpauth.node.mjs node ./test/test.mjs",
    "test:node:esm:min": "JSFILE=../dist/otpauth.node.min.mjs node ./test/test.mjs",
    "test:node:esm": "run-s test:node:esm:*",
    "test:node:cjs:unmin": "JSFILE=../dist/otpauth.node.cjs node ./test/test.mjs",
    "test:node:cjs:min": "JSFILE=../dist/otpauth.node.min.cjs node ./test/test.mjs",
    "test:node:cjs": "run-s test:node:cjs:*",
    "test:node": "run-s test:node:*",
    "test:deno:esm:unmin": "JSFILE=../dist/otpauth.esm.js deno test --allow-read=./dist/ --allow-env=JSFILE --allow-net=deno.land ./test/test.mjs",
    "test:deno:esm:min": "JSFILE=../dist/otpauth.esm.min.js deno test --allow-read=./dist/ --allow-env=JSFILE --allow-net=deno.land ./test/test.mjs",
    "test:deno:esm": "run-s test:deno:esm:*",
    "test:deno": "run-s test:deno:*",
    "test:browser:chromium:umd:unmin": "JSFILE=../dist/otpauth.umd.js BROWSER=chromium node ./test/playwright.mjs",
    "test:browser:chromium:umd:min": "JSFILE=../dist/otpauth.umd.min.js BROWSER=chromium node ./test/playwright.mjs",
    "test:browser:chromium:umd": "run-s test:browser:chromium:umd:*",
    "test:browser:chromium": "run-s test:browser:chromium:*",
    "test:browser:firefox:umd:unmin": "JSFILE=../dist/otpauth.umd.js BROWSER=firefox node ./test/playwright.mjs",
    "test:browser:firefox:umd:min": "JSFILE=../dist/otpauth.umd.min.js BROWSER=firefox node ./test/playwright.mjs",
    "test:browser:firefox:umd": "run-s test:browser:firefox:umd:*",
    "test:browser:firefox": "run-s test:browser:firefox:*",
    "test:browser:webkit:umd:unmin": "JSFILE=../dist/otpauth.umd.js BROWSER=webkit node ./test/playwright.mjs",
    "test:browser:webkit:umd:min": "JSFILE=../dist/otpauth.umd.min.js BROWSER=webkit node ./test/playwright.mjs",
    "test:browser:webkit:umd": "run-s test:browser:webkit:umd:*",
    "test:browser:webkit": "run-s test:browser:webkit:*",
    "test:browser": "run-s test:browser:*",
    "test": "run-s test:*",
    "all": "run-s lint types build docs test",
    "version": "run-s all && git add -A ./types/ ./dist/ ./docs/"
  },
  "dependencies": {
    "jssha": "~3.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.19.3",
    "@babel/eslint-parser": "~7.19.1",
    "@babel/eslint-plugin": "~7.19.1",
    "@babel/preset-env": "~7.19.4",
    "@rollup/plugin-babel": "~6.0.0",
    "@rollup/plugin-node-resolve": "~15.0.0",
    "@rollup/plugin-replace": "~5.0.0",
    "@rollup/plugin-virtual": "~3.0.0",
    "@types/node": "~18.8.5",
    "chai": "~4.3.6",
    "eslint": "~8.25.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-prettier": "~4.2.1",
    "mocha": "~10.0.0",
    "npm-run-all": "~4.1.5",
    "playwright": "~1.27.1",
    "prettier": "~2.7.1",
    "rimraf": "~3.0.2",
    "rollup": "~2.79.1",
    "rollup-plugin-dts": "~4.2.3",
    "rollup-plugin-esbuild": "~4.10.1",
    "test": "~3.2.1",
    "typedoc": "~0.23.16",
    "typescript": "~4.7.4"
  }
}
